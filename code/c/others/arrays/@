#include<stdio.h>
void main()
{
int i,size,j;
printf("enter size of array\n");
scanf("%d",&size);
int a[size];
printf("taking values\n");
for(i=0;i<size;i++)
{
printf("enter value of a[%d]\n",i);
scanf("%d",&a[i]);
}
printf("displaying\n");
for(i=0;i<size;i++)
{
printf("value of a[%d] is %d\n",i,a[i]);
}
printf("finding duplicates\n");
for(i=0;i<size;i++)
{
for(j=0;j<size;j++)
	if(a[i]==a[j])
	{
	printf("a[%d] is a duplicate element and its value is %d\n",i,a[i]);
	break;
	}
}
}

